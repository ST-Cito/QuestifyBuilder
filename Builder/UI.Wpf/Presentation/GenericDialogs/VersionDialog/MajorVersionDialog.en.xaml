<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <s:String x:Key="MajorVersionDialog.Title">New version</s:String>
    <s:String x:Key="MajorVersionDialog.Label">Comments</s:String>
    <s:String x:Key="MajorVersionDialog.Description">Write comments which describe what has been changed in this version.</s:String>

    <s:String x:Key="MajorVersionDialog.Command.Ok">OK</s:String>
    <s:String x:Key="MajorVersionDialog.Command.Cancel">Cancel</s:String>
    <s:String x:Key="MajorVersionDialog.Command.Ok.Error">An error occurred while trying to save. &#x0d;&#x0a;Error: {0}</s:String>
    <s:String x:Key="MajorVersionDialog.MajorVersionDialogViewModel.Remark.Required">Field Comments must be filled.</s:String>
    
    <!-- ViewModel Messages -->
    <s:String x:Key="MajorVersionDialog.MajorVersionDialogViewModel.HasErrors">Not all input is valid.</s:String>
</ResourceDictionary>
