<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">

    <s:String x:Key="Message.WrongCredentials">Invalid combination of username and password.</s:String>
    <s:String x:Key="Message.EmptyUserName">A username is required.</s:String>

    <s:String x:Key="User.Label">Username:</s:String>
    <s:String x:Key="Password.Label">Password:</s:String>
    <s:String x:Key="Login.Header">Login to Questify Builder</s:String>
    
</ResourceDictionary>